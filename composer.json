{
  "name": "idoleg/jwt-auth",
  "description": "JWT Auth Guard for Laravel",
  "homepage": "https://github.com/idoleg/jwt-auth",
  "license": "MIT",
  "authors": [
    {
      "name": "Oleg Shikov",
      "email": "ol.shikov@yandex.ru",
      "role": "Developer"
    }
  ],
  "require": {
    "php" : ">=5.5.0",
    "laravel/framework": "5.5.*",
    "lcobucci/jwt": "^3.2"
  },
  "require-dev": {
    "phpunit/phpunit": "~6.0",
    "mockery/mockery": ">=0.9.9"
  },
  "autoload": {
    "psr-4": {
      "Idoleg\\JwtAuth\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Idoleg\\JwtAuth\\Test\\": "tests"
    }
  },
  "scripts": {
    "test": "phpunit"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Idoleg\\JwtAuth\\Providers\\JwtServiceProvider"
      ]
    }
  }
}
